{
  "ignores": ["node_modules/**"],
  "overrides": [
    {
      "files": ["**/*.js"],
      "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
        "globals": {
          "require": "readonly",
          "module": "readonly",
          "__dirname": "readonly",
          "process": "readonly"
        }
      },
      "rules": {
        "no-unused-vars": "warn",
        "no-console": "off",
        "no-underscore-dangle": "off",
        "no-param-reassign": "off"
      }
    }
  ]
}
